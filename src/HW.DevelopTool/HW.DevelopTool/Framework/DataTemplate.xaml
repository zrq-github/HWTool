<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:HW.DevelopTool.ViewModels"
    xmlns:view="clr-namespace:HW.DevelopTool.Views">
    
    <!-- 将ViewModel 跟数据进行强制绑定 -->
    
    <DataTemplate
        DataType="{x:Type vm:HomeVM}">
        <view:Home></view:Home>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PullProductVM}">
        <view:PullProduct></view:PullProduct>
    </DataTemplate>

</ResourceDictionary>